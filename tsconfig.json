{
"compilerOptions": {
"target": "es6",
"module": "esnext",
"jsx": "react-jsx",
"strict": true,
"moduleResolution": "node",
"esModuleInterop": true,
"forceConsistentCasingInFileNames": true,
"skipLibCheck": true,
"resolveJsonModule": true,
"allowSyntheticDefaultImports": true,
"baseUrl": "./",
"paths": {
"@arcgis/core/*": ["node_modules/@arcgis/core/*"]
}
},
"include": ["src/**/*"],
"exclude": ["node_modules"]
}
